! *utf8: 1
xterm*termName: xterm-256color
! xterm*font: -*-terminus-medium-*-*-*-14-*-*-*-*-*-iso10646-1
xterm*font: -xos4-TerminessPowerline-Medium-R-Normal--14-140-72-72-C-80-ISO10646-1
Xcursor.theme: whiteglass
Xcursor.size: 16

! URxvt.font: -xos4-TerminessPowerline-Medium-R-Normal--14-140-72-72-C-80-ISO10646-1
URxvt.font: -*-terminus-medium-*-*-*-14-*-*-*-*-*-iso10646-1
URxvt.scrollBar: false
! FOR TRUE TRANSPARENCY
! URxvt.depth: 32
! URxvt.background: [95]#000000

URxvt*.transparent: true
! URxvt*.shading: 0 to 99 darkens, 101 to 200 lightens
URxvt*.shading: 40

! *fading:                40
! *fadeColor:             #002b36

xft.dpi:	96
xft.antialias:	true
xft.rgba:	rgb
xft.hinting:	true
xft.hintstyle:	hintslight

